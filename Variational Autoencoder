            ]
          },
          "metadata": {},
          "execution_count": 79
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#train VAE on MNIST dataset\n",
        "vae = VAE()\n",
        "optimizer = Adam(vae.parameters(), lr=0.001)\n",
        "\n",
        "num_epochs = 50\n",
        "for epoch in range(num_epochs):\n",
        "    vae.train()\n",
        "    for batch_idx, (x, _) in enumerate(train_loader):\n",
        "        optimizer.zero_grad()\n",
        "\n",
        "        z, mean, logvar = vae(x)\n",
        "\n",
        "        #Binary Cross Entropy/reconstruction loss\n",
        "        BCE = nn.BCELoss(reduction='sum')(z, x.view(-1, 28*28))\n",
        "        # KL Divergence loss\n",
        "        KLD = -0.5 * torch.sum(1 + logvar - mean**2 - logvar.exp())\n",
        "\n",
        "        loss = BCE + KLD\n",
        "        loss.backward()\n",
        "        optimizer.step()\n",
        "\n",
        "    print(f'Epoch [{epoch + 1}/{num_epochs}] Loss: {loss.item() / len(train_loader.dataset):.3f}')"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
