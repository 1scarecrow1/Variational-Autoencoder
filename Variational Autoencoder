    {
      "cell_type": "code",
      "source": [
        "class Encoder(nn.Module):\n",
        "    def __init__(self):\n",
        "        super().__init__()\n",
        "        self.fc1 = nn.Linear(784, 256) #1st layer with input dim 28x28\n",
        "        self.fc2 = nn.Linear(256, 64)\n",
        "        self.fc3_mean = nn.Linear(64, 8)\n",
        "        self.fc3_var = nn.Linear(64, 8)\n",
        "\n",
        "    def forward(self, x):\n",
        "        x = x.view(-1, 784)  # compress MNIST image\n",
        "        x = torch.relu(self.fc1(x))\n",
        "        x = torch.relu(self.fc2(x))\n",
        "        mean = self.fc3_mean(x) #output 8-dim mean vector\n",
        "        var = self.fc3_var(x)  #output 8-dim positive variance vector\n",
        "        return mean, var"
      ],
      "metadata": {
        "id": "lpKoZcumK84T"
      },
      "execution_count": 63,
      "outputs": []
    },
    {
      "cell_type": "code",
